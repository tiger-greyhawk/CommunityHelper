<Window x:Class="ViewCommunityHelper.View.WindowXaml.OptionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViewCommunityHelper.View.WindowXaml"
        xmlns:view="clr-namespace:ViewCommunityHelper.View.WindowXaml"
        mc:Ignorable="d"
        Title="OptionWindow" Height="300" Width="300">
    <Grid>
        <TabControl>
            <TabItem Header="{DynamicResource OptionsAuthHeader}">
                <view:AuthOptionsUserControl></view:AuthOptionsUserControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
